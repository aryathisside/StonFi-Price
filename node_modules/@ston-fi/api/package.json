{
  "name": "@ston-fi/api",
  "version": "0.24.0",
  "description": "Typescript client for Ston.fi Http API",
  "license": "MIT",
  "homepage": "https://github.com/ston-fi/api#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ston-fi/api.git"
  },
  "type": "module",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "dependencies": {
    "camelcase-keys": "9.1.3",
    "decamelize-keys": "2.0.1",
    "ofetch": "1.4.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@biomejs/biome": "2.0.4",
    "esbuild-analyzer": "0.2.0",
    "lefthook": "1.11.14",
    "publint": "0.3.12",
    "tsup": "8.3.6",
    "typescript": "^5",
    "vitest": "3.2.4"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "cleanup": "rm -rf coverage dist node_modules build-report-*",
    "format": "pnpm run /^format:.*/",
    "format:biome": "biome format --write",
    "lint": "pnpm run /^lint:.*/",
    "lint:tsc": "tsc --noEmit --pretty",
    "lint:biome": "biome check",
    "test": "vitest",
    "build": "tsup"
  }
}